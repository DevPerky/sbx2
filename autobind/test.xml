<Bindings module-name="test">
  <StructSpec name="Test">
    <Members>
      <Parameter type="number" name="testNumber"/>
      <Parameter type="string" name="testString"/>
      <Parameter type="userdata" name="testUserData"/>
    </Members>
  </StructSpec>

  <StructSpec name="Test2">
    <Members>
      <Parameter type="Test" name="customParam"/>
    </Members>
  </StructSpec>

  <StructSpec name="TestIntegers">
    <Members>
      <Parameter type="integer" name="integer"/>
    </Members>
  </StructSpec>

  <FunctionSpec name="testIntegers">
    <In>
      <Parameter type="integer" name="integerIn"/>
    </In>
    <Out>
      <Parameter type="integer" name="integerOut"/>
    </Out>
  </FunctionSpec>
  
  <FunctionSpec name="customTypeIn">
    <In>
      <Parameter type="Test" name="customParam"/>
      <Parameter type="string" name="stringParam"/>
    </In>

    <Out>
      <Parameter type="string" name="outString"/>
      <Parameter type="Test" name="customParamOut"/>
    </Out>

    
  </FunctionSpec>

</Bindings>